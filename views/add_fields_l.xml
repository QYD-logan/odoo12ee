<odoo>
    <!--    tree 视图-->
    <record id="add_label_fields_tree" model="ir.ui.view">
        <field name="name">add.label.fields.tree</field>
        <field name="model">add.label.fields</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="field_description"/>
                <field name="ttype"/>
                <field name="infos"/>
                <field name="readonly"/>
                <!--                <field name="label_line_ids"/>-->
            </tree>
        </field>
    </record>
    <!--    from 视图-->
    <record id="add_label_fields_view_form" model="ir.ui.view">
        <field name="name">add.label.fields.type.view.form</field>
        <field name="model">add.label.fields</field>
        <field name="arch" type="xml">
            <form>
                <heard string="标签字段明细">
                </heard>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="field_description"/>
                            <field name="help"/>
                            <field name="ttype"/>
                            <field name="required"/>
                            <field name="index"/>
                            <field name="domain"/>
                            <field name="groups"/>
                            <field name="store"/>
                        </group>
                        <group>
                            <field name="relation"/>
                            <field name="infos"/>
                            <field name="copied"/>
                            <field name="readonly"/>
                            <!--                            <field name="size"/>-->
                            <field name="state"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <!--    搜索视图(还没写后面再写)-->
    <!--    动作视图,后端调用的这个id-->
    <record id="add_label_fields_action" model="ir.actions.act_window">
        <field name="name">标签字段</field>
        <field name="res_model">add.label.fields</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <!--            <field name="view_id" ref="view_move_line_tree"/>-->
        <!--            <field name="context">{'search_default_done': 1, 'search_default_groupby_product_id': 1}</field>-->
        <!--            <field name="help" type="html">-->
        <!--              <p class="o_view_nocontent_empty_folder">-->
        <!--                There's no product move yet-->
        <!--              </p>-->
        <!--            </field>-->
    </record>

    <!--    <menuitem-->
